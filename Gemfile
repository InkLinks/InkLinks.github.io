  gem "jekyll-sitemap"
  gem "jekyll-mentions"
  gem "jekyll-paginate"
  gem 'jekyll-minibundle'
  gem 'jekyll-theme-balzac'
  gem "jekyll-redirect-from"
  gem "jekyll-language-plugin"
end